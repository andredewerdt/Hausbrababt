                        a(href="winter.html#section-6") View more
                .col-lg-4.col-sm-6.col-xs-6
                  .grid.animated(data-os-animation='fadeInRight')
                    .figure.effect-goliath.winter
                      img(src="images/blokken/garmisch.jpg")
                      figcaption
